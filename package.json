{
  "name": "gcfixer",
  "version": "0.5.0",
  "description": "Normalize Geocaching GPX file for Garmin devices",
  "main": "bin/gcfixer.js",
  "bin": {
    "gcfixer": "./bin/gcfixer.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -p .",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
