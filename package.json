    "unidecode-plus": "^1.0.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.4.2",
    "@types/he": "^1.1.0",
    "@types/html-to-text": "^1.4.31",
    "@types/node": "^13.7.0",
    "@types/yargs": "^15.0.3",
    "eslint": "^6.8.0",
    "patch-package": "^6.2.0",
    "prettier": "^1.19.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.5"
